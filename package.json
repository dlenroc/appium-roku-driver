    "@appium/support": "^4.1.11",
    "@dlenroc/roku": "^1.2.0",
    "@dlenroc/roku-developer-server": "^2.0.0",
    "@dlenroc/roku-ecp": "^2.0.0",
    "@dlenroc/roku-odc": "^2.0.0",
    "asyncbox": "^3.0.0",
    "base-64": "^1.0.0",
    "cssesc": "^3.0.0",
    "jszip": "^3.10.1",
    "roku-dom": "npm:@dlenroc/roku-dom@^2.0.0"
  },
  "devDependencies": {
    "@appium/types": "^0.15.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/base-64": "^1.0.2",
    "@types/cssesc": "^3.0.2",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "libxmljs2": "0.32.0"
