{
  "name": "@dlenroc/appium-roku-driver",
  "version": "0.12.0",
  "description": "Appium Roku Driver",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-roku-driver",
  "keywords": [
    "roku",
    "appium",
    "driver"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "test": "glob 'test/**/*.test.ts' -c 'tsx --test --test-concurrency 1'",
    "watch": "npm run build -- --watch"
  },
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >=24.0.0"
  },
  "appium": {
    "driverName": "roku",
    "automationName": "roku",
    "platformNames": [
      "roku"
    ],
    "mainClass": "Driver"
  },
  "dependencies": {
    "@appium/base-driver": "^10.0.0",
    "@appium/support": "^7.0.0",
    "@dlenroc/roku-debug-server": "^2.0.0",
    "@dlenroc/roku-developer-server": "^2.0.0",
    "@dlenroc/roku-ecp": "^2.1.1",
    "@dlenroc/roku-odc": "^2.1.1",
    "@xmldom/xmldom": "^0.8.11",
    "css-select": "^6.0.0",
    "cssesc": "^3.0.0",
    "hash-object": "^5.0.1",
    "jszip": "^3.10.1",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@appium/types": "^1.0.0",
    "@tsconfig/node20": "^20.1.6",
    "@tsconfig/strictest": "^2.0.5",
    "@types/cssesc": "^3.0.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "webdriverio": "^9.19.1"
  },
  "peerDependencies": {
    "appium": "^2.3.0 || ^3.0.0"
  }
}
