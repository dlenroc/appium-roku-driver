{
  "name": "@dlenroc/appium-roku-driver",
  "version": "0.9.1",
  "description": "Appium Roku Driver",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-roku-driver",
  "keywords": [
    "roku",
    "appium",
    "driver"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run build"
  },
  "engines": {
